  "homepage": "",
  "main": "lib/index",
  "files": [
    "lib/**/*.*"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marshallswain/feathers-unsplash"
  },
  "keywords": [
